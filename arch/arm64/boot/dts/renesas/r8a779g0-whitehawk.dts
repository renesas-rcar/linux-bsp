			csi41_in: endpoint {
				bus-type = <1>;
				clock-lanes = <0>;
				data-lanes = <1 2 3>;
				pin-swap-rx-order = <ABC BCA ABC>;
				hs-receive-eq = <0x4 0x4 0x4>;
				remote-endpoint = <&max96712_out1>;
			};
		};
	};
};

