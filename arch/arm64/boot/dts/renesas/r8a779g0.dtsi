		rcar_sound: sound@ec540000 {
			/*
			 * #sound-dai-cells is required
			 *
			 * Single DAI : #sound-dai-cells = <0>;	<&rcar_sound>;
			 * Multi  DAI : #sound-dai-cells = <1>;	<&rcar_sound N>;
			 */
			/*
			 * #clock-cells is required for audio_clkout
			 *
			 * clkout	: #clock-cells = <0>;	<&rcar_sound>;
			 */
			compatible =  "renesas,rcar_sound-r8a779g0", "renesas,rcar_sound-gen4";
			reg =	<0 0xec540000 0 0x1000>, /* SSIU */
				<0 0xec541000 0 0x050>,  /* SSI */
				<0 0xec5a0000 0 0x020>;  /* ADG */
			reg-names = "ssiu", "ssi", "adg" ;

			clocks = <&cpg CPG_MOD 2926>, <&cpg CPG_MOD 2927>,
				 <&audio_clkin>;
			clock-names = "ssiu.0", "ssi.0",
				      "clkin";
			power-domains = <&sysc R8A779G0_PD_ALWAYS_ON>;
			resets = <&cpg 2926>, <&cpg 2927>;
			reset-names = "ssiu.0", "ssi.0";
			status = "disabled";

			/*
			 * T.B.D
			 * [9. Module Standby, Software Reset] There's only 1 SSI is supported. I think that's why only play or record works.
			 * [Table 30.5 DMA Transfer Requests] There is no information of corresponding SYS-DMAC instance.
			 * I temporarily marked them as sys_dmac0. This point needs to be reconfirmed.
			 *
			*/
			rcar_sound,ssi {
				ssi0: ssi-0 {
					interrupts = <GIC_SPI 258 IRQ_TYPE_LEVEL_HIGH>;
					dmas = <&sys_dmac0 0x6e>, <&sys_dmac0 0x6f>,
					       <&sys_dmac0 0x6c>, <&sys_dmac0 0x6d>,
					       <&sys_dmac0 0x6a>, <&sys_dmac0 0x6b>,
					       <&sys_dmac0 0x68>, <&sys_dmac0 0x69>,
					       <&sys_dmac0 0x66>, <&sys_dmac0 0x67>,
					       <&sys_dmac0 0x64>, <&sys_dmac0 0x65>,
					       <&sys_dmac0 0x62>, <&sys_dmac0 0x63>,
					       <&sys_dmac0 0x60>, <&sys_dmac0 0x61>;
					dma-names = "rx00", "tx00", "rx01", "tx01",
								"rx02", "tx02", "rx03", "tx03",
								"rx04", "tx04", "rx05", "tx05",
								"rx06", "tx06", "rx07", "tx07";
				};
			};
		};

